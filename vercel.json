{
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/search.html",
      "destination": "/search",
      "permanent": true
    },
    {
      "source": "/blog-single.html",
      "has": [
        {
          "type": "query",
          "key": "slug"
        }
      ],
      "destination": "/:slug",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/:slug",
      "destination": "/blog-single.html"
    }
  ]
}
